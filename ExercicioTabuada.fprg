    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="VariavelContador" type="Integer" array="False" size=""/>
            <declare name="VariavelResultado" type="Integer" array="False" size=""/>
            <declare name="VariavelTabuada" type="Integer" array="False" size=""/>
            <output expression="&quot;Entre com a tabuada:&quot;" newline="True"/>
            <input variable="VariavelTabuada"/>
            <assign variable="VariavelContador" expression="0"/>
            <while expression="VariavelContador&lt;=10">
                <assign variable="VariavelResultado" expression="VariavelTabuada*VariavelContador"/>
                <output expression="VariavelTabuada &amp;&quot;N&quot; &amp; VariavelContador &amp;&quot;=&quot; &amp; VariavelResultado" newline="True"/>
                <assign variable="VariavelContador" expression="VariavelContador+1"/>
            </while>
        </body>
